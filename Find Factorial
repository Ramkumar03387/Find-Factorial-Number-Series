import java.util.Scanner;

public class Test extends FindFactorialNumberSeries{
	static int n = 0;
	public static void main(String[] args) {
		FindFactorialNumberSeries object = new FindFactorialNumberSeries();
		objectb.findFact();
	}
	private void findFact() {
		Scanner sc = new Scanner(System.in);
		System.out.println("enter num: ");
		int user = sc.nextInt();
		int total = 1;
		for(int i=1;i<user+1;i++) {
			total = total*i;
		}
		System.out.println(total);
	}
}
